server {
  listen 8080;
  server_name _;
  access_log /mnt/access.log;
  error_log /mnt/error.log;
  root   /var/www/omgubuntu.co.uk;
  index maint.html index.php index.html;

  location = /favicon.ico
  {
    log_not_found off;
    access_log off;
  }

  location = /robots.txt
  {
    allow all;
    log_not_found off;
    access_log off;
  }

  location /nginx_status
  {
    stub_status on;
    access_log   off;
    allow all;
  }

  location ~* \.(js|css|png|jpg|jpeg|gif|ico|zip|gz|tar)$
  {
    expires max;
  }

 location / {
    # This is cool because no php is touched for static content
    try_files /var/www/omgubuntu.co.uk/wp-content/cache/supercache/$uri.html $uri/ $uri @rewrite;
  }                                                                                   

  location @rewrite {
    rewrite ^/(.*)$ /index.php?q=$1;
  }

  client_max_body_size    10m;
  client_body_buffer_size 128k;
  client_header_buffer_size 64k;

  error_page 500 502 503 504  /index.php;
  location ~ \.php$ {
    include /etc/nginx/fastcgi_params;
    fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
    fastcgi_intercept_errors on;
    fastcgi_pass unix:/var/run/php5-fpm.sock;
    #include /etc/nginx/fastcgi_params;
    #fastcgi_pass  127.0.0.1:9000;
    fastcgi_index index.php;
  }
}
